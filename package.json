{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./server/bin/www",
    "postinstall": "gulp build",
    "test": "nyc --reporter=lcov mocha --opts config/mocha.opts --reporter list",
    "coverage": "nyc report",
    "e2e_windows": "npm run e2e_chrome & npm run e2e_ff & npm run e2e_edge",
    "e2e_mac": "npm run e2e_chrome & npm run e2e_ff & npm run e2e_safari",
    "e2e_chrome": "nightwatch --env chrome --config ./config/nightwatch_local.conf.js",
    "e2e_firefox": "nightwatch --config ./config/nightwatch_local.conf.js",
    "e2e_ff": "nightwatch --config ./config/nightwatch_local.conf.js",
    "e2e_edge": "nightwatch --env MicrosoftEdge --config ./config/nightwatch_local.conf.js",
    "e2e_ie": "nightwatch --env ie --config ./config/nightwatch_local.conf.js",
    "e2e_safari": "nightwatch --env safari --config ./config/nightwatch_local.conf.js",
    "e2e_chrome_remote": "nightwatch --env chrome --config ./config/nightwatch_remote.json",
    "e2e_firefox_remote": "nightwatch --config ./config/nightwatch_remote.json",
    "e2e_ff_remote": "nightwatch --config ./config/nightwatch_remote.json",
    "e2e_edge_remote": "nightwatch --env MicrosoftEdge --config ./config/nightwatch_remote.json",
    "e2e_ie_remote": "nightwatch --env ie11 --config ./config/nightwatch_remote.json",
    "e2e_safari_remote": "nightwatch --env safari --config ./config/nightwatch_remote.json",
    "e2e_remote": "nightwatch --env chrome,firefox,safari,edge,ie11 --config ./config/nightwatch_remote.json",
    "sendCoverage": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "bin",
      "coverage",
      "test/*"
    ]
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.3",
    "debug": "~2.6.3",
    "ejs": "~2.5.6",
    "express": "~4.15.2",
    "jwt-simple": "^0.5.1",
    "mongoose": "^4.9.9",
    "mongoose-encryption": "^1.5.0",
    "morgan": "~1.8.1",
    "nodemailer": "^4.0.1",
    "serve-favicon": "~2.4.2",
    "slack-notify": "^0.1.6",
    "socket.io": "^2.0.1",
    "stripe": "^4.19.0",
    "twilio": "^3.0.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.18.8",
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.1",
    "gulp": "^3.9.1",
    "gulp-apidoc": "^0.2.6",
    "gulp-babel": "^6.1.2",
    "gulp-clean-css": "^3.2.0",
    "gulp-connect": "^5.0.0",
    "gulp-ejs": "^3.0.1",
    "gulp-eslint": "^3.0.1",
    "gulp-htmlhint": "^0.3.1",
    "gulp-htmlmin": "^3.0.0",
    "gulp-imagemin": "^3.2.0",
    "gulp-jsdoc3": "^1.0.1",
    "gulp-jshint": "^2.0.4",
    "gulp-mocha": "^4.3.0",
    "gulp-newer": "^1.3.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-stylelint": "^3.9.0",
    "gulp-uglify": "^2.1.2",
    "gulp-util": "^3.0.8",
    "htmlhint": "^0.9.13",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.4",
    "mocha": "^3.2.0",
    "nightwatch": "^0.9.14",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.3.0",
    "require-dir": "^0.3.1",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "supertest": "^3.0.0"
  }
}
